<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.261" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller3">
   <children>
      <VBox alignment="CENTER" layoutX="10.0" layoutY="47.0" prefHeight="411.0" prefWidth="580.0">
         <children>
            <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <TextField fx:id="titleField" alignment="CENTER" promptText="title" />
                  <TextField fx:id="isbnField" promptText="isbn" />
                  <TextArea fx:id="authorField" prefHeight="80.0" prefWidth="183.0" promptText="firstName newLine lastName">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </TextArea>
                  <TextField fx:id="priceField" promptText="price" />
               </children>
            </HBox>
            <HBox alignment="CENTER" prefHeight="52.0" prefWidth="560.0" spacing="10.0">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <Button mnemonicParsing="false" onAction="#insert" text="INSERT">
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#search" text="SEARCH" />
                  <Button mnemonicParsing="false" onAction="#delete" text="DELETE" />
                  <Button mnemonicParsing="false" onAction="#update" text="UPDATE" />
               </children>
            </HBox>
            <TextArea fx:id="textArea" prefHeight="198.0" prefWidth="400.0" promptText="console" VBox.vgrow="ALWAYS" />
         </children>
      </VBox>
   </children>
</AnchorPane>
